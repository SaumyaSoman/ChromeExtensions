{
  "manifest_version": 2,

  "name": "MarkPageRed",
  "description": "Turns color to red",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "Click here!"
  },
  "background": {
    "scripts": ["popup.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ]
}